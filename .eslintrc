{
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": true
  },
  "extends": ["airbnb-base", "airbnb-base/legacy", "plugin:jest/recommended"],
  "env": {
    "browser": true,
    "node": true,
    "jest/globals": true
  },
  "rules": {
    "arrow-parens": "off",
    "comma-dangle": "off",
    "consistent-return": "off",
    "func-names": "off",
    "no-console": "off"
  }
}
